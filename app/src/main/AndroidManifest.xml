<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.startup.pattern" >




        <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:theme="@style/MyMaterialTheme" >
            <activity
                android:name="com.startup.pattern.MainActivity"
                android:label="@string/app_name" >
                <intent-filter>
                    <action android:name="android.intent.action.MAIN" />

                    <category android:name="android.intent.category.LAUNCHER" />
                </intent-filter>
            </activity>
            <activity
                android:name="com.startup.pattern.ScrollableTabsActivity"
                android:label="@string/title_activity_scrollable_tabs"
                android:parentActivityName="com.startup.pattern.MainActivity" >
            </activity>

        </application>

    </manifest>
